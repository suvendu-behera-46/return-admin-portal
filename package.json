{
  "name": "return-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:merge": "prisma-tools merge --schema=./prisma/schema.prisma",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "migrate": "npx dotenv -e .env.local -- prisma migrate deploy",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/x-date-pickers": "^6.20.0",
    "@prisma/client": "^5.12.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.11",
    "exceljs": "^4.4.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "next": "14.1.4",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.9.13",
    "pg": "^8.12.0",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.12.7",
    "sass": "^1.72.0",
    "scss": "^0.2.4"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.1",
    "prisma": "^5.12.0",
    "typescript": "5.4.5"
  }
}
